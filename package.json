{
  "name": "turbo-vets",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:api": "nx build api",
    "build:dashboard": "nx build dashboard",
    "serve:api": "nx serve api",
    "serve:dashboard": "nx serve dashboard",
    "lint": "nx run-many --target=lint --all",
    "test": "nx run-many --target=test --all",
    "test:api": "nx test api",
    "test:dashboard": "nx test dashboard",
    "e2e:api": "nx e2e api-e2e",
    "e2e:dashboard": "nx e2e dashboard-e2e",
    "prisma:generate": "nx run api:prisma-generate",
    "prisma:migrate": "nx run api:prisma-migrate",
    "prisma:studio": "nx run api:prisma-studio",
    "db:seed": "npx prisma db seed --schema=apps/api/prisma/schema.prisma",
    "db:push": "npx prisma db push --schema=apps/api/prisma/schema.prisma",
    "start:api": "nx serve api",
    "start:app": "nx serve dashboard",
    "dev:api": "npx ts-node --project apps/api/tsconfig.app.json -r dotenv/config apps/api/src/main.ts",
    "dev:app": "nx serve dashboard --port=4200",
    "run:dev": "npm run prisma:generate && npx concurrently -k \"nx serve api\" \"nx serve dashboard --port=4200\""
  },
  "prisma": {
    "seed": "ts-node apps/api/prisma/seed.ts"
  },
